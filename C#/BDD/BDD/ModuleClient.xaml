<Window x:Class="BDD.ModuleClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BDD"
        mc:Ignorable="d"
        
        WindowStartupLocation="CenterScreen"
        Title="ModuleClient" Height="160" Width="300">
    <StackPanel>
        <Button Name ="ConnexionButton" Content="Connexion" Click="Connexion_Click" FontWeight="Medium" FontStyle="Italic"/>
        <Button Name ="LoginButton" Content="Sign Up Client" Click="Login_Click" FontWeight="Medium" FontStyle="Italic"/>
        <Button Name ="PasserCommandeButton" Content="Passer une commande" Click="PasserUneCommande_Click" FontWeight="Medium" FontStyle="Italic"/>
        <Button Name ="DeconnexionButton" Content="Deconnexion" Click="Deconnexion_Click" FontWeight="Medium" FontStyle="Italic"/>
        <Button Name ="SupprimerCompteButton" Content="Supprimer compte" Click="Supprimer_Click" FontWeight="Medium" FontStyle="Italic"/>
        <Button Name ="RetourButton" Content="Retour" Click="RetourButton_Click" FontWeight="Medium" FontStyle="Italic"/>
    </StackPanel>
</Window>
