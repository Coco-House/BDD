<Window x:Class="BDD.ModuleClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BDD"
        mc:Ignorable="d"
        Title="ModuleClient" Height="120" Width="300">
    <StackPanel>
        <Button x:Name ="LoginButton" Content="Login Client" Click="Login_Click" />
        <Button x:Name ="PasserCommandeButton" Content="Passer une commande" Click="PasserUneCommande_Click" />
        <Button x:Name ="DeconnexionButton" Content="Deconnexion" Click="Deconnexion_Click" />
        <Button x:Name ="RetourButton" Content="Retour" Click="RetourButton_Click" />
    </StackPanel>
</Window>
